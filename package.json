        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.6",
        "express": "^4.17.3",
        "nodemon": "^2.0.15",
        "peer": "^0.6.1",
        "peerjs": "^1.3.2",
        "socket.io": "^4.4.1",
        "socket.io-client": "^4.4.1",
