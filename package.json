        "fs-extra": "^10.0.1",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.5",
        "ts-node-dev": "^1.1.8",
        "tsc": "^2.0.4",
        "tslint": "^6.1.3",
        "typescript": "^4.5.5"
