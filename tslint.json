{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [false]
    },
    "rulesDirectory": [
        "src/server.ts"
    ],
    "linterOptions": {
        "exclude": [
            "src/public/*.ts",
            "src/public/*.tsx",
            "src/public/*.js",
            "src/public/*.jsx"
        ]
    }
}